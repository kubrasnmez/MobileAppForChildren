<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff980"
    tools:context=".duyuorgan">
    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_duyuorganlar">
    </include>
    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout5"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginTop="225dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/burun"
            android:layout_width="115dp"
            android:layout_height="115dp"
            android:layout_margin="5dp"
            android:onClick="burun"
            android:src="@drawable/burundetay"
            app:civ_border_color="#b600b9"
            app:civ_border_width="3dp"
            app:layout_column="0"
            app:layout_row="1" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/agiz"
            android:layout_width="115dp"
            android:layout_height="115dp"
            android:layout_margin="5dp"
            android:onClick="agiz"
            android:src="@drawable/agizdetay"
            app:civ_border_color="#00e8de"
            app:civ_border_width="3dp"
            app:layout_column="1"
            app:layout_row="1" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/goz"
            android:layout_width="115dp"
            android:layout_height="115dp"
            android:layout_margin="5dp"
            android:onClick="goz"
            android:src="@drawable/gozdetay"
            app:civ_border_color="#ff3072"
            app:civ_border_width="3dp"
            app:layout_column="0"
            app:layout_row="2" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/deri"
            android:layout_width="115dp"
            android:layout_height="115dp"
            android:layout_margin="5dp"
            android:onClick="deri"
            android:src="@drawable/eldetay"
            app:civ_border_color="#ff1e28"
            app:civ_border_width="3dp"
            app:layout_column="1"
            app:layout_row="2" />
    </androidx.gridlayout.widget.GridLayout>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/kulak"
        android:layout_width="115dp"
        android:layout_height="115dp"
        android:layout_marginTop="105dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toTopOf="@+id/gridLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@drawable/kulakdetay"
        app:civ_border_color="#10e84c"
        app:civ_border_width="3dp"
        android:onClick="kulak"/>
    <androidx.gridlayout.widget.GridLayout
        android:layout_width="200dp"
        android:layout_height="99dp"
        android:layout_marginBottom="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/home"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:onClick="home"
            app:layout_row="0"
            app:layout_column="0"
            android:layout_margin="10dp"
            app:srcCompat="@drawable/homepembe" />
        <ImageView
            android:id="@+id/ses"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:onClick="ses"
            app:layout_row="0"
            app:layout_column="1"
            android:layout_margin="10dp"
            app:srcCompat="@drawable/sespembe" />
    </androidx.gridlayout.widget.GridLayout>


</androidx.constraintlayout.widget.ConstraintLayout>