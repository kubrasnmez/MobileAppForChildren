<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#febcff"
    tools:context=".kisgiysi">
    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_kisgiysi">
    </include>
    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayout6"
        android:layout_width="322dp"
        android:layout_height="322dp"
        android:layout_marginTop="110dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/mont"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:onClick="mont"
            android:src="@drawable/mont_menu"
            app:civ_border_width="3dp"
            android:layout_margin="5dp"
            app:layout_column="0"
            app:layout_row="0" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/sapka"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:onClick="sapka"
            android:src="@drawable/sapka_menu"
            app:civ_border_width="3dp"
            android:layout_margin="5dp"
            app:layout_column="1"
            app:layout_row="0" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/atki"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:onClick="atki"
            android:src="@drawable/atki_menu"
            app:civ_border_width="3dp"
            android:layout_margin="5dp"
            app:layout_column="2"
            app:layout_row="0" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/corap"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:onClick="corap"
            android:src="@drawable/corap_menu"
            app:civ_border_width="3dp"
            android:layout_margin="5dp"
            app:layout_column="0"
            app:layout_row="1" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/hirka"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:onClick="hirka"
            android:src="@drawable/hirka_menu"
            app:civ_border_width="3dp"
            android:layout_margin="5dp"
            app:layout_column="1"
            app:layout_row="1" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/eldiven"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:onClick="eldiven"
            android:layout_margin="5dp"
            android:src="@drawable/eldiven_menu"
            app:civ_border_width="3dp"
            app:layout_column="2"
            app:layout_row="1" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/semsiye"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:layout_margin="5dp"
            android:onClick="semsiye"
            android:src="@drawable/semsiye_menu"
            app:civ_border_width="3dp"
            app:layout_column="0"
            app:layout_row="2" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/bot"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:layout_margin="5dp"
            android:onClick="bot"
            android:src="@drawable/bot_menu"
            app:civ_border_width="3dp"
            app:layout_column="1"
            app:layout_row="2" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/kazak"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:layout_margin="5dp"
            android:onClick="kazak"
            android:src="@drawable/kazak_menu"
            app:civ_border_width="3dp"
            app:layout_column="2"
            app:layout_row="2" />


    </androidx.gridlayout.widget.GridLayout>
    <androidx.gridlayout.widget.GridLayout
        android:id="@+id/gridLayoutmenu"
        android:layout_width="214dp"
        android:layout_height="107dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/home"
            android:layout_width="83dp"
            android:layout_height="83dp"
            android:layout_margin="13dp"
            android:onClick="home"
            app:layout_column="0"
            app:layout_row="0"
            app:srcCompat="@drawable/home" />

        <ImageView
            android:id="@+id/ses"
            android:layout_width="83dp"
            android:layout_height="83dp"
            android:layout_margin="13dp"
            android:onClick="ses"
            app:layout_column="1"
            app:layout_row="0"
            app:srcCompat="@drawable/sesyesil" />
    </androidx.gridlayout.widget.GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>